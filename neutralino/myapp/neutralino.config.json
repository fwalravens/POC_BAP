{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/base_app/dist",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*"
  ],
  "modes": {
    "window": {
      "title": "myapp",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/base_app/public/favicon.ico"
    }
  },
  "cli": {
    "binaryName": "myapp",
    "resourcesPath": "/base_app/dist/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/base_app/public/neutralino.js",
    "binaryVersion": "4.5.0",
    "clientVersion": "3.4.0",
    "frontendLibrary": {
      "patchFile": "/base_app/public/index.html",
      "devUrl": "http://localhost:3000"
    }
  }
}